@model ResetPasswordViewModel
@{
    ViewData["Title"] = "ResetPassword";
    Layout = "~/Views/Shared/AuthLayout.cshtml";
}



<div class="main-agileinfo">
	<div class="agileits-top">
		<form asp-action="ResetPassword" method="post">
			<input class="text" asp-for="NewPassword" placeholder="new Password" required="">
			<input class="text w3lpass" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">
			
			<input type="submit" value="reset password">
		</form>
		<p>Do have an Account? <a asp-action="Login"> Login Now!</a></p>
	</div>
</div>

